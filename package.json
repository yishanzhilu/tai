  "private": true,
  "scripts": {
    "build": "next build",
    "build:analyzer": "ANALYZE=true next build",
    "dev": "next -p 10001",
    "git-cz": "git-cz -s",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "lint:fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "start": "NODE_ENV=production next start -p 10000",
    "test": "jest --onlyChanged",
    "test:full": "jest --coverage",
    "test:updateSnapshot": "jest --onlyChanged --updateSnapshot",
    "verify": "npm-run-all -p test:full lint",
    "updatePkgCache": "node ./scripts/updatePkgCache && git add ./package.cache.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all updatePkgCache lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
